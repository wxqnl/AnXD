{"ast":null,"code":"import { ref, reactive, onMounted } from 'vue';\nimport { ElMessage, ElMessageBox } from 'element-plus';\nimport { getCouponList, createCoupon, updateCoupon, deleteCoupon, changeCouponStatus } from '@/api/coupon';\n\n// 查询参数\n\nexport default {\n  __name: 'Coupon',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const queryParams = reactive({\n      pageNum: 1,\n      pageSize: 10,\n      name: '',\n      type: undefined,\n      status: undefined\n    });\n\n    // 表格数据和状态\n    const couponList = ref([]);\n    const loading = ref(false);\n    const total = ref(0);\n\n    // 获取优惠券列表\n    const getList = async () => {\n      try {\n        loading.value = true;\n        const res = await getCouponList(queryParams);\n        console.log('优惠券列表响应:', res);\n        couponList.value = res.data.items || [];\n        total.value = res.data.total || 0;\n      } catch (error) {\n        console.error('获取优惠券列表失败', error);\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // 查询按钮\n    const handleQuery = () => {\n      queryParams.pageNum = 1;\n      getList();\n    };\n\n    // 重置查询\n    const resetQuery = () => {\n      queryParams.name = '';\n      queryParams.type = undefined;\n      queryParams.status = undefined;\n      queryParams.pageNum = 1;\n      getList();\n    };\n\n    // 获取券类型文本\n    const getTypeText = type => {\n      const typeMap = {\n        1: '满减券',\n        2: '折扣券',\n        3: '现金券'\n      };\n      return typeMap[type] || '未知';\n    };\n\n    // 格式化优惠券金额/折扣\n    const formatCouponValue = row => {\n      if (row.type === 1 || row.type === 3) {\n        return `￥${row.amount}`;\n      } else if (row.type === 2) {\n        return `${row.amount * 10}折`;\n      }\n      return row.amount;\n    };\n\n    // 格式化使用门槛\n    const formatCondition = row => {\n      if (row.type === 3) {\n        return '无门槛';\n      } else {\n        return row.condition > 0 ? `满${row.condition}元可用` : '无门槛';\n      }\n    };\n\n    // 获取状态文本\n    const getStatusText = status => {\n      const statusMap = {\n        0: '未开始',\n        1: '进行中',\n        2: '已结束',\n        3: '已关闭'\n      };\n      return statusMap[status] || '未知';\n    };\n\n    // 获取状态对应的标签类型\n    const getStatusType = status => {\n      const typeMap = {\n        0: 'info',\n        1: 'success',\n        2: 'danger',\n        3: 'warning'\n      };\n      return typeMap[status] || '';\n    };\n\n    // 新增优惠券\n    const handleAdd = () => {\n      ElMessage.info('新增优惠券功能待实现');\n    };\n\n    // 编辑优惠券\n    const handleEdit = row => {\n      ElMessage.info(`编辑优惠券: ${row.name}`);\n    };\n\n    // 启用/停用优惠券\n    const handleStatus = row => {\n      const statusText = row.status === 1 ? '停用' : '启用';\n      ElMessageBox.confirm(`确定要${statusText}优惠券 ${row.name} 吗?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        try {\n          await changeCouponStatus({\n            id: row.id,\n            status: row.status === 1 ? 3 : 1\n          });\n          ElMessage.success(`${statusText}成功`);\n          getList();\n        } catch (error) {\n          ElMessage.error(`${statusText}失败`);\n        }\n      }).catch(() => {});\n    };\n\n    // 删除优惠券\n    const handleDelete = row => {\n      ElMessageBox.confirm(`确定要删除优惠券 ${row.name} 吗?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        try {\n          await deleteCoupon(row.id);\n          ElMessage.success('删除成功');\n          getList();\n        } catch (error) {\n          ElMessage.error('删除失败');\n        }\n      }).catch(() => {});\n    };\n\n    // 分页操作\n    const handleSizeChange = size => {\n      queryParams.pageSize = size;\n      getList();\n    };\n    const handleCurrentChange = page => {\n      queryParams.pageNum = page;\n      getList();\n    };\n\n    // 页面加载时获取数据\n    onMounted(() => {\n      getList();\n    });\n    const __returned__ = {\n      queryParams,\n      couponList,\n      loading,\n      total,\n      getList,\n      handleQuery,\n      resetQuery,\n      getTypeText,\n      formatCouponValue,\n      formatCondition,\n      getStatusText,\n      getStatusType,\n      handleAdd,\n      handleEdit,\n      handleStatus,\n      handleDelete,\n      handleSizeChange,\n      handleCurrentChange,\n      ref,\n      reactive,\n      onMounted,\n      get ElMessage() {\n        return ElMessage;\n      },\n      get ElMessageBox() {\n        return ElMessageBox;\n      },\n      get getCouponList() {\n        return getCouponList;\n      },\n      get createCoupon() {\n        return createCoupon;\n      },\n      get updateCoupon() {\n        return updateCoupon;\n      },\n      get deleteCoupon() {\n        return deleteCoupon;\n      },\n      get changeCouponStatus() {\n        return changeCouponStatus;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","ElMessage","ElMessageBox","getCouponList","createCoupon","updateCoupon","deleteCoupon","changeCouponStatus","queryParams","pageNum","pageSize","name","type","undefined","status","couponList","loading","total","getList","value","res","console","log","data","items","error","handleQuery","resetQuery","getTypeText","typeMap","formatCouponValue","row","amount","formatCondition","condition","getStatusText","statusMap","getStatusType","handleAdd","info","handleEdit","handleStatus","statusText","confirm","confirmButtonText","cancelButtonText","then","id","success","catch","handleDelete","handleSizeChange","size","handleCurrentChange","page"],"sources":["/Users/apple/Desktop/111111/zongheshixi/daijiaguanli/frontui/src/pages/coupon/Coupon.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <el-card class=\"search-card\">\n      <el-form :model=\"queryParams\" ref=\"queryForm\" :inline=\"true\" class=\"search-form\">\n        <el-form-item label=\"优惠券名称\" prop=\"name\">\n          <el-input v-model=\"queryParams.name\" placeholder=\"请输入优惠券名称\" clearable />\n        </el-form-item>\n        <el-form-item label=\"券类型\" prop=\"type\">\n          <el-select v-model=\"queryParams.type\" placeholder=\"请选择券类型\" clearable>\n            <el-option label=\"满减券\" :value=\"1\" />\n            <el-option label=\"折扣券\" :value=\"2\" />\n            <el-option label=\"现金券\" :value=\"3\" />\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"状态\" prop=\"status\">\n          <el-select v-model=\"queryParams.status\" placeholder=\"请选择状态\" clearable>\n            <el-option label=\"未开始\" :value=\"0\" />\n            <el-option label=\"进行中\" :value=\"1\" />\n            <el-option label=\"已结束\" :value=\"2\" />\n            <el-option label=\"已关闭\" :value=\"3\" />\n          </el-select>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"handleQuery\">查询</el-button>\n          <el-button @click=\"resetQuery\">重置</el-button>\n        </el-form-item>\n      </el-form>\n    </el-card>\n\n    <el-card>\n      <template #header>\n        <div class=\"card-header\">\n          <span>优惠券列表</span>\n          <el-button type=\"primary\" @click=\"handleAdd\">新增优惠券</el-button>\n        </div>\n      </template>\n      \n      <el-table v-loading=\"loading\" :data=\"couponList\" border>\n        <el-table-column type=\"index\" label=\"#\" width=\"50\" />\n        <el-table-column prop=\"name\" label=\"优惠券名称\" />\n        <el-table-column prop=\"type\" label=\"券类型\">\n          <template #default=\"scope\">\n            {{ getTypeText(scope.row.type) }}\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"amount\" label=\"优惠金额/折扣\">\n          <template #default=\"scope\">\n            {{ formatCouponValue(scope.row) }}\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"condition\" label=\"使用门槛\">\n          <template #default=\"scope\">\n            {{ formatCondition(scope.row) }}\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"validity\" label=\"有效期\">\n          <template #default=\"scope\">\n            {{ scope.row.startTime }} 至 {{ scope.row.endTime }}\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"stock\" label=\"库存\" width=\"80\" />\n        <el-table-column prop=\"usedCount\" label=\"已使用\" width=\"80\" />\n        <el-table-column prop=\"status\" label=\"状态\">\n          <template #default=\"scope\">\n            <el-tag :type=\"getStatusType(scope.row.status)\">\n              {{ getStatusText(scope.row.status) }}\n            </el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作\" width=\"180\">\n          <template #default=\"scope\">\n            <el-button type=\"primary\" link @click=\"handleEdit(scope.row)\">编辑</el-button>\n            <el-button type=\"success\" link @click=\"handleStatus(scope.row)\" v-if=\"scope.row.status !== 2\">\n              {{ scope.row.status === 1 ? '停用' : '启用' }}\n            </el-button>\n            <el-button type=\"danger\" link @click=\"handleDelete(scope.row)\">删除</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n      \n      <el-pagination\n        v-if=\"total > 0\"\n        v-model:current-page=\"queryParams.pageNum\"\n        v-model:page-size=\"queryParams.pageSize\"\n        :page-sizes=\"[10, 20, 50, 100]\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n      />\n    </el-card>\n  </div>\n</template>\n\n<script setup>\nimport { ref, reactive, onMounted } from 'vue'\nimport { ElMessage, ElMessageBox } from 'element-plus'\nimport { getCouponList, createCoupon, updateCoupon, deleteCoupon, changeCouponStatus } from '@/api/coupon'\n\n// 查询参数\nconst queryParams = reactive({\n  pageNum: 1,\n  pageSize: 10,\n  name: '',\n  type: undefined,\n  status: undefined\n})\n\n// 表格数据和状态\nconst couponList = ref([])\nconst loading = ref(false)\nconst total = ref(0)\n\n// 获取优惠券列表\nconst getList = async () => {\n  try {\n    loading.value = true\n    const res = await getCouponList(queryParams)\n    console.log('优惠券列表响应:', res)\n    couponList.value = res.data.items || []\n    total.value = res.data.total || 0\n  } catch (error) {\n    console.error('获取优惠券列表失败', error)\n  } finally {\n    loading.value = false\n  }\n}\n\n// 查询按钮\nconst handleQuery = () => {\n  queryParams.pageNum = 1\n  getList()\n}\n\n// 重置查询\nconst resetQuery = () => {\n  queryParams.name = ''\n  queryParams.type = undefined\n  queryParams.status = undefined\n  queryParams.pageNum = 1\n  getList()\n}\n\n// 获取券类型文本\nconst getTypeText = (type) => {\n  const typeMap = {\n    1: '满减券',\n    2: '折扣券',\n    3: '现金券'\n  }\n  return typeMap[type] || '未知'\n}\n\n// 格式化优惠券金额/折扣\nconst formatCouponValue = (row) => {\n  if (row.type === 1 || row.type === 3) {\n    return `￥${row.amount}`\n  } else if (row.type === 2) {\n    return `${row.amount * 10}折`\n  }\n  return row.amount\n}\n\n// 格式化使用门槛\nconst formatCondition = (row) => {\n  if (row.type === 3) {\n    return '无门槛'\n  } else {\n    return row.condition > 0 ? `满${row.condition}元可用` : '无门槛'\n  }\n}\n\n// 获取状态文本\nconst getStatusText = (status) => {\n  const statusMap = {\n    0: '未开始',\n    1: '进行中',\n    2: '已结束',\n    3: '已关闭'\n  }\n  return statusMap[status] || '未知'\n}\n\n// 获取状态对应的标签类型\nconst getStatusType = (status) => {\n  const typeMap = {\n    0: 'info',\n    1: 'success',\n    2: 'danger',\n    3: 'warning'\n  }\n  return typeMap[status] || ''\n}\n\n// 新增优惠券\nconst handleAdd = () => {\n  ElMessage.info('新增优惠券功能待实现')\n}\n\n// 编辑优惠券\nconst handleEdit = (row) => {\n  ElMessage.info(`编辑优惠券: ${row.name}`)\n}\n\n// 启用/停用优惠券\nconst handleStatus = (row) => {\n  const statusText = row.status === 1 ? '停用' : '启用'\n  ElMessageBox.confirm(`确定要${statusText}优惠券 ${row.name} 吗?`, '提示', {\n    confirmButtonText: '确定',\n    cancelButtonText: '取消',\n    type: 'warning'\n  }).then(async () => {\n    try {\n      await changeCouponStatus({\n        id: row.id,\n        status: row.status === 1 ? 3 : 1\n      })\n      ElMessage.success(`${statusText}成功`)\n      getList()\n    } catch (error) {\n      ElMessage.error(`${statusText}失败`)\n    }\n  }).catch(() => {})\n}\n\n// 删除优惠券\nconst handleDelete = (row) => {\n  ElMessageBox.confirm(`确定要删除优惠券 ${row.name} 吗?`, '提示', {\n    confirmButtonText: '确定',\n    cancelButtonText: '取消',\n    type: 'warning'\n  }).then(async () => {\n    try {\n      await deleteCoupon(row.id)\n      ElMessage.success('删除成功')\n      getList()\n    } catch (error) {\n      ElMessage.error('删除失败')\n    }\n  }).catch(() => {})\n}\n\n// 分页操作\nconst handleSizeChange = (size) => {\n  queryParams.pageSize = size\n  getList()\n}\n\nconst handleCurrentChange = (page) => {\n  queryParams.pageNum = page\n  getList()\n}\n\n// 页面加载时获取数据\nonMounted(() => {\n  getList()\n})\n</script>\n\n<style scoped>\n.app-container {\n  padding: 20px;\n}\n\n.search-card {\n  margin-bottom: 20px;\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.el-pagination {\n  margin-top: 20px;\n  text-align: right;\n}\n</style> "],"mappings":"AA+FA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,QAAQ,KAAI;AAC7C,SAASC,SAAS,EAAEC,YAAY,QAAQ,cAAa;AACrD,SAASC,aAAa,EAAEC,YAAY,EAAEC,YAAY,EAAEC,YAAY,EAAEC,kBAAkB,QAAQ,cAAa;;AAEzG;;;;;;;;IACA,MAAMC,WAAW,GAAGT,QAAQ,CAAC;MAC3BU,OAAO,EAAE,CAAC;MACVC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAEC,SAAS;MACfC,MAAM,EAAED;IACV,CAAC;;IAED;IACA,MAAME,UAAU,GAAGjB,GAAG,CAAC,EAAE;IACzB,MAAMkB,OAAO,GAAGlB,GAAG,CAAC,KAAK;IACzB,MAAMmB,KAAK,GAAGnB,GAAG,CAAC,CAAC;;IAEnB;IACA,M